<% if Rails.configuration.branding[:keys].present? %>

  <!-- Environment specific -->
  <% if Rails.env.stage? %>
    <script type="text/javascript">
      (function() { var s = document.createElement("script"); s.type = "text/javascript"; s.async = true; s.src = '//api.usersnap.com/load/<%= Rails.configuration.branding[:keys][:usersnap_key] %>.js'; var x = document.getElementsByTagName('script')[0]; x.parentNode.insertBefore(s, x); })();
    </script>
  <% end %>
<% end %>
